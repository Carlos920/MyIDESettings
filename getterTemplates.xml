<application>
  <component name="GetterTemplates" defaultTemplateName="isGetter">
    <option name="templates">
      <list>
        <TemplateResource>
          <option name="fileName" value="isGetter" />
          <option name="template" value="#if($field.modifierStatic)&#10;static ##&#10;#end&#10;$field.type ##&#10;#set($name = $StringUtil.capitalizeWithJavaBeanConvention($field.name))&#10;get##&#10;${name}() {&#10;return $field.name;&#10;}" />
        </TemplateResource>
      </list>
    </option>
  </component>
</application>